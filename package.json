    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.0.2",
    "enzyme": "^2.8.2",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "jsdom": "^9.0.0",
    "mocha": "^3.4.2",
    "react": "^15.6.0",
    "react-addons-test-utils": "^15.5.1",
